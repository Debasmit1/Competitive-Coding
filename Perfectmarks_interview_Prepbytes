def Perfect_Number(Number):
    Sum = 0
    for i in range(1, Number):
        if(Number % i == 0):
            Sum = Sum + i
    return Sum        

    

n = int(input())

for i in range(n):
  num = int(input())
  
  if(num == Perfect_Number(num)):
    print("true")
  else:
    print('false')
    
